{
  "manifest_version": 3,
  "name": "Image Fetcher",
  "version": "1.0",
  "description": "Fetches images from the current webpage and displays them in the extension popup.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
